    "@prisma/client": "^2.5.1",
    "apollo-server-micro": "^2.16.1",
    "aws-sdk": "^2.739.0",
    "cookie": "^0.4.1",
    "crypto": "^1.0.1",
    "graphql": "^15.3.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-tools": "^6.1.0",
    "next": "9.5.2",
    "next-images": "^1.4.1",
    "nexus": "^0.26.1",
    "nexus-plugin-prisma": "^0.17.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
